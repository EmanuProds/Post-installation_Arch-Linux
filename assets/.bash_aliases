# Custom aliases

alias in='paru -S'
alias ds='paru -Rsc'
#alias rin='sudo dnf reinstall -y'
alias pr='paru -Ss'
alias fin='flatpak install flathub -y'
alias fds='flatpak remove -y'
alias att='paru -Syuu -y'
alias fatt='flatpak update -y'
alias tatt='paru -Syuu -y && flatpak update -y'
alias rtatt='paru -Syuu -y && flatpak update -y && reboot'
alias dtatt='paru -Syuu -y -y && flatpak update -y && shutdown'
alias sys='neofetch'
